<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
 	<class name="com.Sconit.Entity.Transportation.TransportationAddress, com.Sconit.Entity" table="TAddress" lazy="false">
		<id name="Id" column="Id" type="Int32" unsaved-value="0" >
      		<generator class="identity" />
        </id>
		<property name="Country" column="Country" type="String" length="50" />	
		<property name="Province" column="Province" type="String" length="50" />	
		<property name="City" column="City" type="String" length="50" />	
		<property name="District" column="District" type="String" length="50" />	
		<property name="Address" column="Address" type="String" length="50" />	
  	</class>
</hibernate-mapping>
