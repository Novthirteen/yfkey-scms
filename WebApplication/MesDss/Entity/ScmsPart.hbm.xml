<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
 	<class name="com.Mes.Dss.Entity.ScmsPart, com.Mes.Dss" table="SCMS_PART" lazy="false">
		<id name="Code" column="PART_NUMBER" type="String" unsaved-value="null" length="64">
      		<generator class="assigned" />
        </id>
        <property name="Flag" column="INTER_FLAG" type="Int32"  />
        <property name="Des" column="PART_DES" type="String" length="255" />
        <property name="Uom" column="MEASUREMENT" type="String" length="64" />
        <property name="LastModifyDate" column="LAST_MODIFIED_TIME" type="DateTime"  />
        <property name="LastModifyUser" column="LAST_MODIFIED_USER" type="String" length="64"/>
    </class>
</hibernate-mapping>
